<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="ParallelExecution" parallel="tests" thread-count="3">
    <test name="User could register with valid data - Firefox">
        <parameter name="browsername" value="firefox"/>
        <classes>
            <class name="testcases.TC02_RegisterTest">
                <methods>
                    <include name="registerWithValidData_P"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="User could register with valid data - Edge">
        <parameter name="browsername" value=""/>
        <classes>
            <class name="testcases.TC02_RegisterTest">
                <methods>
                    <include name="registerWithValidData_P"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="User could register with valid data - Chrome">
        <parameter name="browsername" value="chrome-headless"/>
        <classes>
            <class name="testcases.TC02_RegisterTest">
                <methods>
                    <include name="registerWithValidData_P"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>